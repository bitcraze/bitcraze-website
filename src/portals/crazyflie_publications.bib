journal = {2017 22nd International Conference on Methods and Models in Automation and Robotics, MMAR 2017},
keywords = {Crazyflie 2.0,autonomous flights,autonomous landing,flight simulator,nano quadcopter,nano quadrotor helicopter,obstacles avoidance,vision-based tracking},
month = {sep},
pages = {37--42},
publisher = {Institute of Electrical and Electronics Engineers Inc.},
title = {{Crazyflie 2.0 quadrotor as a platform for research and education in robotics and control engineering}},
url = {https://www.youtube.com/watch?v=BQJHj0SBhys https://www.youtube.com/watch?v=c2HkxI_KMcY https://www.youtube.com/watch?v=3r1ks7B15AA https://www.youtube.com/watch?v=IAwlL2Y9XHA},
year = {2017}
