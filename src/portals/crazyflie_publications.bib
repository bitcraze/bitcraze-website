journal = {Sensors (Switzerland)},
keywords = {Drone,Electronics,Gas distribution mapping,Gas sensors,Gas source localization,MOX sensor,Quadcopter,Robotics,Signal processing,UAV},
month = {jan},
number = {3},
pages = {478},
pmid = {30682827},
publisher = {Multidisciplinary Digital Publishing Institute},
title = {{Smelling nano aerial vehicle for gas source localization and mapping}},
url = {https://www.mdpi.com/1424-8220/19/3/478/htm https://www.mdpi.com/1424-8220/19/3/478},
